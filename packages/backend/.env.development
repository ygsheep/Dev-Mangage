# 数据库配置
DATABASE_URL="file:./dev.db"

# 服务器配置
PORT=3320
NODE_ENV=development

# JWT配置（如果需要认证）
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# API配置
API_PREFIX=/api/v1

# CORS配置 - 允许本地和局域网连接
CORS_ORIGIN=*

# 文件上传配置
MAX_FILE_SIZE=10485760  # 10MB
UPLOAD_DIR=./uploads

# Swagger配置
SWAGGER_ENABLED=true

# 日志配置
LOG_LEVEL=debug